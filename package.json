{
  "name": "moneo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently --raw w: npm:start:*",
    "start:next": "next dev",
    "start:nexus": "cross-env REFLECTION=true ts-node-dev --respawn --transpile-only server/schema.ts",
    "start:apollo": "apollo client:codegen --target typescript --localSchemaFile schema.graphql --outputFlat --watch src/apollo",
    "build": "next build",
    "prepare": "prisma generate"
  },
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@prisma/client": "^2.18.0",
    "apollo-server-micro": "^2.21.1",
    "micro": "^9.3.4",
    "next": "^10.0.9",
    "nexus": "^1.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.2.0",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "apollo": "^2.32.5",
    "babel-plugin-graphql-tag": "^3.2.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "prisma": "^2.18.0",
    "ts-node-dev": "^1.1.6",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "typescript": "^4.2.3"
  }
}

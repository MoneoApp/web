{
  "name": "moneo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --raw npm:dev:*",
    "dev:next": "next dev",
    "dev:nexus": "cross-env REFLECTION=true ts-node-dev --respawn --transpile-only --quiet server/schema.ts",
    "dev:apollo": "apollo client:codegen --target typescript --localSchemaFile schema.graphql --outputFlat --watch src/apollo",
    "build": "next build",
    "prestart": "prisma migrate deploy",
    "start": "next start",
    "prepare": "prisma generate",
    "test": "tslint --project tsconfig.json && jest",
    "migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@hookform/resolvers": "^2.4.0",
    "@prisma/client": "^2.21.2",
    "apollo-server-micro": "^2.23.0",
    "bcryptjs": "^2.4.3",
    "dakpan": "^2.3.1",
    "dialoog": "^1.0.0",
    "graphql-scalars": "^1.9.1",
    "jsonwebtoken": "^8.5.1",
    "micro": "^9.3.4",
    "next": "^10.1.3",
    "nexus": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.3.4",
    "superstruct": "^0.15.1",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/jest": "^11.3.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^15.0.0",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/validator": "^13.1.3",
    "apollo": "^2.32.10",
    "babel-plugin-graphql-tag": "^3.2.0",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "next-images": "^1.7.0",
    "prisma": "^2.21.2",
    "ts-node-dev": "^1.1.6",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "typescript": "^4.2.4"
  },
  "jest": {
    "snapshotSerializers": [
      "@emotion/jest/serializer"
    ]
  }
}

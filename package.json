{
  "name": "moneo",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": "7.x"
  },
  "scripts": {
    "dev": "concurrently --raw npm:dev:*",
    "dev:next": "next dev",
    "dev:nexus": "cross-env REFLECTION=true ts-node-dev --respawn --transpile-only server/schema.ts",
    "dev:apollo": "apollo client:codegen --target typescript --localSchemaFile schema.graphql --outputFlat --watch src/apollo",
    "start": "next start -p $PORT",
    "build": "next build",
    "prepare": "prisma generate"
  },
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@prisma/client": "^2.18.0",
    "apollo-server-micro": "^2.21.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.5.1",
    "micro": "^9.3.4",
    "next": "^10.0.9",
    "nexus": "^1.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "apollo": "^2.32.5",
    "babel-plugin-graphql-tag": "^3.2.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "prisma": "^2.18.0",
    "ts-node-dev": "^1.1.6",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "typescript": "^4.2.3"
  },
  "cacheDirectories": [
    "node_modules",
    ".next/cache"
  ]
}

{
  "name": "moneo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --raw npm:dev:*",
    "dev:next": "next dev",
    "dev:nexus": "cross-env REFLECTION=true ts-node-dev --respawn --transpile-only --quiet server/schema.ts",
    "dev:apollo": "apollo client:codegen --target typescript --localSchemaFile schema.graphql --outputFlat --watch src/apollo",
    "build": "next build",
    "prestart": "prisma migrate deploy",
    "start": "next start",
    "prepare": "prisma generate",
    "test": "tslint --project tsconfig.json && jest",
    "migrate": "prisma migrate dev",
    "preinstall": "npx npm-force-resolutions"
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@hookform/resolvers": "^2.5.1",
    "@prisma/client": "^2.23.0",
    "apollo-server-micro": "^2.24.1",
    "apollo-upload-client": "^15.0.0",
    "bcryptjs": "^2.4.3",
    "csshake": "^1.5.3",
    "dakpan": "^2.3.1",
    "date-fns": "^2.21.3",
    "dialoog": "^1.0.0",
    "file-type": "^16.4.0",
    "fuse.js": "^6.4.6",
    "graphql-scalars": "^1.9.3",
    "jsonwebtoken": "^8.5.1",
    "konva": "^7.2.5",
    "micro": "^9.3.4",
    "nanoid": "^3.1.23",
    "next": "^10.2.0",
    "nexus": "^1.0.0",
    "nodemailer": "^6.6.0",
    "prisma": "^2.23.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.6.2",
    "react-konva": "^17.0.2-0",
    "superstruct": "^0.15.2",
    "use-image": "^1.0.7",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/jest": "^11.3.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^6.0.0",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^15.3.0",
    "@types/nodemailer": "^6.4.1",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/validator": "^13.1.3",
    "apollo": "^2.33.2",
    "babel-plugin-graphql-tag": "^3.2.0",
    "concurrently": "^6.1.0",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "ts-node-dev": "^1.1.6",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "fs-capacitor": "^6.2.0"
  },
  "jest": {
    "snapshotSerializers": [
      "@emotion/jest/serializer"
    ]
  }
}
